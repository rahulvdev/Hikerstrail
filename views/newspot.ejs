<% include ./partials/header %>

<div class="container">
    <div>
 <form action="/hikespots" method="POST">
  <div class="form-group">
    <label>Camp Name</label>
    <input type="input" class="form-control"  name="hikesite[name]" placeholder="Hikespot Name">
  </div>
  <div class="form-group">
    <label>Image Url</label>
    <input type="input" class="form-control" name="hikesite[image]" placeholder="Image Url">
  </div>
  <div class="form-group">
      <label>Hikespot Description</label>
      <textarea rows="10" cols="10" name="hikesite[description]"></textarea>
  </div>
  <button type="submit" class="btn btn-primary">Submit</button>
</form>
</div>
</div>

<% include ./partials/footer %>